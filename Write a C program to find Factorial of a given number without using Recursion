#include <stdio.h>

int main() {
    int n;
    unsigned long long fact = 1;  

    // Input
    printf("Enter a number: ");
    scanf("%d", &n);

    // Factorial using loop
    for (int i = 1; i <= n; i++) {
        fact *= i;
    }

    // Output
    printf("Factorial of %d = %llu\n", n, fact);

    return 0;
}
