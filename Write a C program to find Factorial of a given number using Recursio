#include <stdio.h>

// Recursive function for factorial
long long factorial(int n) {
    if (n == 0 || n == 1)
        return 1;   // base case
    else
        return n * factorial(n - 1);  // recursive call
}

int main() {
    int num;

    // Input
    printf("Enter a number: ");
    scanf("%d", &num);

    // Output
    printf("Factorial of %d = %lld\n", num, factorial(num));

    return 0;
}
